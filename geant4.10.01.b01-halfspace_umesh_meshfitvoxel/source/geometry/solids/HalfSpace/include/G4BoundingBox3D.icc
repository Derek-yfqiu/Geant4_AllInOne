inline G4ThreeVector G4BoundingBox3D::GetBoxMin() const
{
  return box_min;
}


inline G4ThreeVector G4BoundingBox3D::GetBoxMax() const
{
  return box_max;
}


inline G4double G4BoundingBox3D::GetDistance() const 
{
  return distance;
}


inline void G4BoundingBox3D::SetDistance(G4double distance0) 
{
  distance = distance0;
}


inline G4int G4BoundingBox3D::GetTestResult() const 
{
  return test_result;
}

inline G4ThreeVector G4BoundingBox3D::GetMiddlePoint() const
{
    return MiddlePoint;
}

inline G4double G4BoundingBox3D::GetSize() const
{
    return size;
}

inline G4double G4BoundingBox3D::GetDX() const
{
    return Dx;
}
inline G4double G4BoundingBox3D::GetDY() const
{
    return Dy;
}
inline G4double G4BoundingBox3D::GetDZ() const
{
    return Dz;
}
